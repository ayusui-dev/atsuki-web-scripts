{
  "name": "atsuki-web-project",
  "version": "1.0.0",
  "scripts": {
    "obfuscate": "powershell -Command \"Get-ChildItem ../atsuki-web-system/site/js -Recurse -Filter *.js | ForEach-Object { uglifyjs $_.FullName -o $_.FullName --compress --mangle }\"",
    "serve": "node ../atsuki-web-system/server.js",
    "open": "start http://localhost:3000",
    "buildJs":"webpack --config webpack.config.js",
    "generate-html": "ts-node --project ./tsconfig.json -r tsconfig-paths/register ./build-module/outputHtmls.ts",
    "build:release": "npm run buildJs && npm run generate-html"
  }
}
